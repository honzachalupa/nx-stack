{
    "name": "nx-stack",
    "version": "0.0.4",
    "license": "MIT",
    "private": true,
    "scripts": {
        "start": "NODE_ENV=development nx serve",
        "start:prod": "NODE_ENV=production nx serve",
        "start:firebase-functions": "yarn build:dev && firebase emulators:start",
        "start:e2e": "concurrently \"nx serve\" \"cd packages/apps/react-app-e2e && npx cypress open && cd ../../../\" -n \"App,Cypress\"",
        "build:dev": "NODE_ENV=development nx build && NODE_ENV=development nx run firebase-functions:build && yarn run-scripts",
        "build:prod": "NODE_ENV=production nx build && NODE_ENV=production nx run firebase-functions:build && yarn run-scripts",
        "lint": "nx affected --target=lint",
        "test": "nx affected --target=test",
        "test-e2e": "nx e2e react-app-e2e",
        "code:check": "yarn lint && yarn test && yarn build:dev",
        "run-scripts": "node tools/scripts/generate-firebase-functions-package-json.js",
        "local:link": "node ./tools/scripts/local.link.js",
        "local:unlink": "node ./tools/scripts/local.unlink.js",
        "storybooks:watch": "nx run-many --target=storybook --all",
        "storybooks:build": "NODE_ENV=production nx run-many --target=build-storybook --all",
        "storybooks:publish": "yarn storybooks:build && npx chromatic --storybook-build-dir=dist/storybooks/react-app --exit-once-uploaded --allow-console-errors --no-interactive && npx chromatic --storybook-build-dir=dist/storybooks/react-app --exit-once-uploaded --allow-console-errors --no-interactive",
        "update:libs": "yarn upgrade --pattern @honzachalupa --latest",
        "release": "standard-version",
        "postinstall": "npx husky install",
        "temp": "NODE_ENV=production && echo $NODE_ENV && dotenv -p NX_FIREBASE_API_KEY"
    },
    "dependencies": {
        "@honzachalupa/firebase": "^1.16.1",
        "@honzachalupa/ui": "^1.16.1",
        "@honzachalupa/utils": "^1.16.1",
        "core-js": "^3.6.5",
        "cors": "^2.8.5",
        "express": "^4.18.1",
        "favicons": "^6.2.2",
        "firebase-admin": "^10.2.0",
        "firebase-functions": "^3.18.1",
        "node-fetch": "2",
        "react": "17.0.2",
        "react-collapsible": "^2.10.0",
        "react-dom": "17.0.2",
        "react-helmet": "^6.1.0",
        "react-is": "17.0.2",
        "react-markdown": "^8.0.1",
        "react-pro-sidebar": "^0.7.1",
        "react-router-dom": "^6.2.2",
        "react-social-login-buttons": "^3.6.0",
        "regenerator-runtime": "0.13.7",
        "rehype-raw": "^6.1.1",
        "remark-gfm": "^3.0.1",
        "styled-components": "5.3.3",
        "tslib": "^2.0.0",
        "use-broadcast": "^0.1.2",
        "use-deep-compare-effect": "^1.8.1",
        "use-react-router-breadcrumbs": "^3.1.0",
        "uuid-by-string": "^3.0.6"
    },
    "devDependencies": {
        "@commitlint/config-conventional": "^16.2.1",
        "@faker-js/faker": "^6.0.0",
        "@nrwl/cli": "13.8.5",
        "@nrwl/cypress": "13.8.5",
        "@nrwl/eslint-plugin-nx": "13.8.5",
        "@nrwl/jest": "13.8.5",
        "@nrwl/linter": "13.8.5",
        "@nrwl/nx-cloud": "latest",
        "@nrwl/react": "13.8.5",
        "@nrwl/tao": "13.8.5",
        "@nrwl/web": "13.8.5",
        "@nrwl/workspace": "13.8.5",
        "@storybook/addon-essentials": "~6.4.18",
        "@storybook/addon-storyshots": "^6.4.18",
        "@storybook/builder-webpack5": "~6.4.18",
        "@storybook/manager-webpack5": "~6.4.18",
        "@storybook/react": "~6.4.18",
        "@testing-library/react": "12.1.2",
        "@testing-library/react-hooks": "7.0.2",
        "@types/jest": "27.0.2",
        "@types/node": "16.11.7",
        "@types/react": "17.0.30",
        "@types/react-dom": "17.0.9",
        "@types/react-helmet": "^6.1.5",
        "@types/react-is": "17.0.2",
        "@types/styled-components": "5.1.15",
        "@typescript-eslint/eslint-plugin": "~5.10.0",
        "@typescript-eslint/parser": "~5.10.0",
        "babel-jest": "27.2.3",
        "babel-loader": "^8.2.5",
        "chromatic": "^6.6.3",
        "commitlint": "^16.2.1",
        "concurrently": "^7.1.0",
        "cypress": "^10.1.0",
        "dotenv": "^16.0.1",
        "dotenv-cli": "^6.0.0",
        "eslint": "~8.7.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jsx-a11y": "6.5.1",
        "eslint-plugin-react": "7.28.0",
        "eslint-plugin-react-hooks": "4.3.0",
        "favicons-webpack-plugin": "^5.0.2",
        "husky": "^7.0.4",
        "jest": "27.2.3",
        "prettier": "^2.5.1",
        "react-test-renderer": "17.0.2",
        "standard-version": "^9.3.2",
        "ts-jest": "27.0.5",
        "typescript": "~4.5.2",
        "webpack-bundle-analyzer": "^4.5.0",
        "workbox-webpack-plugin": "^6.5.1"
    }
}
